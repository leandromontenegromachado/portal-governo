<?xml version="1.0" encoding="utf-8"?>
<registry>
  <records interface="plone.base.interfaces.controlpanel.IMailSchema"
           prefix="plone"
           purge="false"
  >
    <value key="email_from_name">leandro-machado@procergr.rs.gov.br</value>
  </records>
  <record field="email_from_address"
          interface="plone.base.interfaces.controlpanel.IMailSchema"
          name="plone.email_from_address"
  >
    <field type="plone.registry.field.ASCIILine">
      <description xmlns:ns0="http://xml.zope.org/namespaces/i18n"
                   ns0:domain="plone"
                   ns0:translate=""
      >Plone generates e-mail using this address as the e-mail return address. It is also used as the destination address for the site-wide contact form and the 'Send test e-mail' feature.</description>
      <title xmlns:ns0="http://xml.zope.org/namespaces/i18n"
             ns0:domain="plone"
             ns0:translate=""
      >Site 'From' address</title>
    </field>
    <value>leandro-machado@procergs.rs.gov.br</value>\
  </record>
  <record field="email_from_name"
          interface="plone.base.interfaces.controlpanel.IMailSchema"
          name="plone.email_from_name"
  >
    <field type="plone.registry.field.TextLine">
      <description xmlns:ns0="http://xml.zope.org/namespaces/i18n"
                   ns0:domain="plone"
                   ns0:translate=""
      >Plone generates e-mail using this name as the e-mail sender.</description>
      <title xmlns:ns0="http://xml.zope.org/namespaces/i18n"
             ns0:domain="plone"
             ns0:translate=""
      >Site 'From' name</title>
    </field>
    <value>portal-governo</value>
  </record>
